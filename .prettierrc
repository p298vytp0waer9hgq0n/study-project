{
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "importOrder": ["<THIRD_PARTY_MODULES>", "^\\..*(?<!css)$", "css$"],
    "importOrderSeparation": true
}
